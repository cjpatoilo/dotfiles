{
  "name": "dotfiles",
  "version": "1.0.1",
  "description": "A restore point for sync your settings and preferences in your toolbox.",
  "homepage": "https://cjpatoilo.com/dotfiles",
  "repository": "cjpatoilo/dotfiles",
  "license": "MIT",
  "author": "CJ Patoilo <cjpatoilo@gmail.com>",
  "bin": "src/index.md",
  "keywords": [
    "boilerplate",
    "opensource",
    "repository",
    "starterkit",
    "github"
  ],
  "ignore": [
    "artwork.psd",
    ".appveyor.yml",
    ".editorconfig",
    ".github",
    ".gitignore",
    ".travis.yml"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^2.4.0",
    "boeing": "latest",
    "editorconfig-tools": "^0.1.1",
    "husky": "^3.0.8",
    "marshmallow": "latest",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0"
  },
  "engines": {
    "node": ">10.0.0"
  },
  "scripts": {
    "prepush": "marshmallow -o docs -i artwork.psd -t Dotfiles -d 'A restore point for sync your settings and preferences in your toolbox.' -f -m && boeing docs && rimraf docs",
    "precommit": "npm t",
    "test": "nyc ava && editorconfig-tools check ."
  }
}
